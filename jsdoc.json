{
  "recurseDepth": 10,
  "source": {
    "include": ["src/"],
    "includePattern": "\\.js$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "encoding": "utf8",
    "template": "custom-template"
  }
}